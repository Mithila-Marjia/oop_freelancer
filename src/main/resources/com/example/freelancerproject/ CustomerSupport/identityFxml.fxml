<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.freelancerproject. CustomerSupport.IdentityFxml">
   <children>
      <Label fx:id="identityLable" layoutX="127.0" layoutY="6.0" prefHeight="35.0" prefWidth="293.0" text="IDENTITY VERIFICATION">
         <font>
            <Font name="System Bold Italic" size="24.0" />
         </font>
      </Label>
      <TextField fx:id="searchTextField" layoutX="14.0" layoutY="46.0" prefHeight="25.0" prefWidth="458.0" promptText="Search" />
      <TableView fx:id="freelancerTableView" layoutX="14.0" layoutY="100.0" prefHeight="299.0" prefWidth="322.0">
        <columns>
          <TableColumn fx:id="nameColumn" prefWidth="108.0" text="Freelancer" />
          <TableColumn fx:id="dateColumn" prefWidth="115.0" text="Submitted" />
            <TableColumn fx:id="statusColumn" prefWidth="98.0" text="Status" />
        </columns>
      </TableView>
      <Label fx:id="freelancerdetails" layoutX="429.0" layoutY="92.0" text="Freelancer Details">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <TextArea fx:id="identitydocumenttTextArea" layoutX="372.0" layoutY="122.0" prefHeight="96.0" prefWidth="200.0" promptText="identityDocument" />
      <Button layoutX="382.0" layoutY="231.0" mnemonicParsing="false" onAction="#approveButton" prefHeight="39.0" prefWidth="96.0" text="Approve">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button layoutX="489.0" layoutY="231.0" mnemonicParsing="false" onAction="#rejectButton" text="Reject">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
