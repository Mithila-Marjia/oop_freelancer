<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.freelancerproject.CustomerSupport.FeedbackFxml">
   <children>
      <Label fx:id="userLabel" layoutX="211.0" layoutY="14.0" text="USER FEEDBACK">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <TableView fx:id="feedbackTableView" layoutX="-1.0" layoutY="78.0" prefHeight="322.0" prefWidth="304.0">
        <columns>
          <TableColumn fx:id="feedbackidTableColumn" prefWidth="146.0" text="Feedback ID" />
          <TableColumn fx:id="subjectTableColumn" prefWidth="157.0" text="Subject" />
        </columns>
      </TableView>
      <TextArea fx:id="analysisTextArea" layoutX="361.0" layoutY="185.0" prefHeight="109.0" prefWidth="200.0" promptText="Analysis result" />
      <Button layoutX="415.0" layoutY="308.0" mnemonicParsing="false" onAction="#exportButton" text="EXPORT">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
      <Label fx:id="feedbackLabel" layoutX="361.0" layoutY="144.0" text="Feedback Analysis">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label fx:id="feedbackidLabel" layoutX="340.0" layoutY="92.0" text="Feedback ID:">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <ComboBox fx:id="feedbackidCombobox" layoutX="433.0" layoutY="74.0" prefHeight="55.0" prefWidth="166.0" promptText="ID" />
      <Button layoutX="516.0" layoutY="347.0" mnemonicParsing="false" onAction="#backButton" prefHeight="35.0" prefWidth="81.0" text="Back">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
