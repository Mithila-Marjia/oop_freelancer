<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.freelancerproject.CustomerSupport.MonitorFxml">
   <children>
      <Label fx:id="contentLable" layoutX="168.0" layoutY="11.0" text="CONTENT MODERATION">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Label fx:id="contenIDtLable" layoutX="14.0" layoutY="61.0" text="Content ID:">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <TableView fx:id="monitorcontentTableView" layoutY="114.0" prefHeight="286.0" prefWidth="383.0">
        <columns>
          <TableColumn fx:id="contentidTableColumn" prefWidth="99.0" text="Content ID" />
          <TableColumn fx:id="contentnameTableColumn" prefWidth="161.0" text="Content Name" />
            <TableColumn fx:id="statusTableColumn" prefWidth="116.0" text="Status" />
        </columns>
      </TableView>
      <Button layoutX="415.0" layoutY="290.0" mnemonicParsing="false" onAction="#saveButton" prefHeight="30.0" prefWidth="71.0" text="Save">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
      <Button layoutX="501.0" layoutY="290.0" mnemonicParsing="false" onAction="#ignoreButton" text="IGNORE">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
      <ComboBox fx:id="contentidcombobox" layoutX="99.0" layoutY="59.0" prefHeight="45.0" prefWidth="275.0" promptText="ID" />
      <Button layoutX="520.0" layoutY="345.0" mnemonicParsing="false" onAction="#backButton" prefHeight="35.0" prefWidth="71.0" text="Back">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
