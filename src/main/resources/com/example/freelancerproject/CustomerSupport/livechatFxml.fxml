<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="messageidCombbobox" prefHeight="350.0" prefWidth="526.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.freelancerproject.CustomerSupport.LivechatFxml">
   <children>
      <Label fx:id="liveLable" layoutX="224.0" layoutY="14.0" text="LIVE CHAT">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <TextField fx:id="typeTextField" layoutX="18.0" layoutY="218.0" prefHeight="60.0" prefWidth="330.0" promptText="Type a message" />
      <Button layoutX="380.0" layoutY="229.0" mnemonicParsing="false" onAction="#sendButton" prefHeight="25.0" prefWidth="96.0" text="SEND">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
      <ComboBox fx:id="messageidcombobox" layoutX="21.0" layoutY="76.0" prefHeight="39.0" prefWidth="173.0" promptText="message id" />
      <TextField fx:id="senderidTextField" editable="false" layoutX="21.0" layoutY="123.0" prefHeight="25.0" prefWidth="173.0" promptText="sender id" />
      <TextArea fx:id="messageTextArea" editable="false" layoutX="19.0" layoutY="157.0" prefHeight="52.0" prefWidth="457.0" promptText="message" />
      <Button layoutX="436.0" layoutY="289.0" mnemonicParsing="false" onAction="#backButton" prefHeight="45.0" prefWidth="76.0" text="Back">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
