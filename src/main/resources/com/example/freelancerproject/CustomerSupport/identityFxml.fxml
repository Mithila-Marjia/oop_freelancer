<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.freelancerproject.CustomerSupport.IdentityFxml">
   <children>
      <Label fx:id="identityLabel" layoutX="127.0" layoutY="6.0" prefHeight="35.0" prefWidth="293.0" text="IDENTITY VERIFICATION">
         <font>
            <Font name="System Bold Italic" size="24.0" />
         </font>
      </Label>
      <TableView fx:id="identityTableView" layoutX="-1.0" layoutY="58.0" prefHeight="341.0" prefWidth="337.0">
        <columns>
          <TableColumn fx:id="freelanceridTablaColumn" prefWidth="108.0" text="Freelancer ID" />
          <TableColumn fx:id="nameTableColumn" prefWidth="115.0" text="Name" />
            <TableColumn fx:id="pricingTableColumn" prefWidth="113.0" text="Pricing" />
        </columns>
      </TableView>
      <Label fx:id="freelancerdetailsLabel" layoutX="372.0" layoutY="144.0" text="Freelancer Details">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <TextArea fx:id="identitydocumenttTextArea" layoutX="372.0" layoutY="174.0" prefHeight="96.0" prefWidth="227.0" promptText="identityDocument" />
      <Button layoutX="372.0" layoutY="283.0" mnemonicParsing="false" onAction="#approveButton" prefHeight="39.0" prefWidth="109.0" text="Approve">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
      <Button layoutX="499.0" layoutY="283.0" mnemonicParsing="false" onAction="#rejectButton" text="Reject">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
      <Label fx:id="freelanceridLabel" layoutX="358.0" layoutY="58.0" text="Freelancer ID:">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Label>
      <ComboBox fx:id="freelanceridcombobox" layoutX="443.0" layoutY="54.0" prefHeight="49.0" prefWidth="158.0" promptText="ID" />
      <Button layoutX="499.0" layoutY="343.0" mnemonicParsing="false" onAction="#backButton" prefHeight="39.0" prefWidth="96.0" text="Back">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
