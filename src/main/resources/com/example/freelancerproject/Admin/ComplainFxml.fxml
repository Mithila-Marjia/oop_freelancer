          <TableColumn fx:id="idTableColumn" prefWidth="83.0" text="ID" />
          <TableColumn fx:id="freelencerTableColumn" prefWidth="78.0" text="Freelancer" />
            <TableColumn fx:id="clintTableColumn" prefWidth="88.0" text="Client" />
