<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="446.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.freelancerproject.Admin.ReportFxml">
   <children>
      <Label fx:id="generateLable" layoutX="236.0" layoutY="22.0" text="Generate Reports " textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Label fx:id="reportLable" layoutX="4.0" layoutY="153.0" prefHeight="27.0" prefWidth="113.0" text="Report Type:">
         <font>
            <Font name="System Bold" size="18.0" />
         </font></Label>
      <Label fx:id="fromdateLable" layoutX="2.0" layoutY="204.0" prefHeight="27.0" prefWidth="96.0" text="From Date:">
         <font>
            <Font name="System Bold" size="18.0" />
         </font></Label>
      <Label fx:id="descriptionLable" layoutX="16.0" layoutY="288.0" prefHeight="27.0" prefWidth="121.0" text="Description:">
         <font>
            <Font name="System Bold" size="18.0" />
         </font></Label>
      <ComboBox fx:id="reportTypeCombobox" layoutX="126.0" layoutY="141.0" prefHeight="41.0" prefWidth="262.0" promptText="Select a report" />
      <DatePicker fx:id="fromDatePicker" layoutX="126.0" layoutY="200.0" prefHeight="41.0" prefWidth="262.0" promptText="mm/dd/yyyy" />
      <TextArea fx:id="descriptionTextArea" layoutX="126.0" layoutY="257.0" prefHeight="116.0" prefWidth="262.0" promptText="text">
         <font>
            <Font size="18.0" />
         </font></TextArea>
      <Button layoutX="408.0" layoutY="345.0" mnemonicParsing="false" onAction="#generstereportButton" text="Generate Report">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Label fx:id="idLable" layoutX="4.0" layoutY="84.0" text="ID:">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="reportidTextField" layoutX="126.0" layoutY="77.0" prefHeight="41.0" prefWidth="262.0" promptText="report id">
         <font>
            <Font size="13.0" />
         </font>
      </TextField>
      <Button layoutX="489.0" layoutY="392.0" mnemonicParsing="false" onAction="#backButton" prefHeight="41.0" prefWidth="96.0" text="Back">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
